{
    "name": "nimblersoft/drupal_consul",
    "description": "An open citizenship participation platform built as a Drupal distribution, curated by Nimblersoft",
    "type": "drupal-profile",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8",
        "drush/drush": "9.0.0-beta7",
        "drupal/drupal-extension": "~3.3.0",
        "behat/behat": "^3.0",
        "drupal/coder": "~8.2.0",
        "phing/phing": "^2.14",
        "squizlabs/php_codesniffer": "2.*",
        "se/selenium-server-standalone": "^2.53",
        "drupal/console": "^1.0.0",
        "jakoch/phantomjs-installer": "1.9.8",
        "composer/composer": "^1.4",
        "grasmash/yaml-cli": "^1.0",
        "consolidation/robo": "1.1.4",
        "drupal/media_entity_generic": "1.x-dev"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "scripts": {
        "post-install-cmd": [
        ],
        "post-update-cmd": [
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    },
    "extra": {
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "docroot/profiles/drupal_consul/modules/contrib/{$name}": [
                "type:drupal-module"
            ]
        }
    },
    "require": {
        "cweagans/composer-patches": "^1.6.4",
        "drupal/core": "~8.4.3",
        "drupal/admin_toolbar": "^1.21",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/ctools": "^3.0",
        "drupal/devel": "^1.2",
        "drupal/ds": "^3.1",
        "drupal/entity_reference_revisions": "^1.3",
        "drupal/field_permissions": "^1.0@beta",
        "drupal/flag": "^4.0@alpha",
        "drupal/fontawesome": "^1.3",
        "drupal/fontawesome_menu_icons": "^1.2",
        "drupal/libraries": "3.x-dev",
        "drupal/masquerade": "^2.0@beta",
        "drupal/paragraphs": "^1.2",
        "drupal/pathauto": "^1.0",
        "drupal/poll": "^1.1",
        "drupal/profile": "^1.0@RC",
        "drupal/select_registration_roles": "^1.2",
        "drupal/smart_trim": "^1.0",
        "drupal/social_api": "^1.1",
        "drupal/social_auth": "^1.0",
        "drupal/social_auth_facebook": "^1.0",
        "drupal/token": "^1.0",
        "drupal/token_filter": "^1.0@beta",
        "drupal/votingapi": "^3.0@alpha",
        "drupal/votingapi_widgets": "^1.0@alpha",
        "drupal/bootstrap": "^3.7"
    }
}
